"""
This is an empty file for the Pi Camera
"""
import time
import picamera
with picamera.PiCamera() as camera:
    camera.start_preview()
    try:
        for i, filename in enumerate(camera.capture_continuous('image{counter:03d}.jpg')):
            print(filename)
            time.sleep(4)
            if i == 75:
                break
    finally:
        camera.stop_preview()
